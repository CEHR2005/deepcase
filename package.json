{
  "name": "@deepcase/deepgraph-app",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "unsafe-dev": "next dev -p $PORT",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "dev": "export HASURA_PATH=$(echo $(gp url 8080) | awk -F[/:] '{print $4}'); export HASURA_SSL=1; export PORT=3002; export $(grep -v '^#' .env | xargs); npx next unsafe-dev -p $PORT"
  },
  "dependencies": {
    "@deepcase/deepgraph": "^0.0.4",
    "dotenv": "^8.2.0",
    "next": "^10.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "engines": {
    "node": "^14.15.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "dotenv-webpack": "^7.0.2",
    "webpack": "^5.28.0"
  }
}
