  "main": "build/src/index.js",
  "scripts": {
    "build": "tsc",
    "electron:start-live": "node ./live-runner.js",
    "electron:start": "npm run build && electron --inspect=5858 ./",
    "electron:pack": "npm run build && electron-builder -c ./electron-builder.config.json",
    "electron:sign": "npm run electron:sign-prod && npm run electron:sign-dev",
    "electron:sign-prod": "electron-osx-sign dist/mas/Deep.Case.app --entitlements=./entitlements.mas.plist --entitlements-inherit=./entitlements.mas.inherit.plist --identity='Apple Distribution: ABRAKAR, OOO (8ZG6TUT4AY)' --platform=mas --type=distribution --provisioning-profile=deepcaseappstore.provisionprofile",
    "electron:sign-dev": "electron-osx-sign dist/mas-dev/Deep.Case.app --identity='Apple Development: Пётр Безденежных (2H45PHYHQ5)' --platform=mas --type=development --provisioning-profile=./deepcasedevelopment.provisionprofile",
    "electron:make": "npm run build && electron-builder build -c ./electron-builder.config.json -p always"
  },
  "dependencies": {
    "@capacitor-community/electron": "^4.0.1",
    "@deep-foundation/deeplinks": "0.0.36",
    "@deep-foundation/hasura": "0.0.10",
    "@deep-foundation/materialized-path": "0.0.8",
    "@deep-foundation/store": "0.0.5",
    "chokidar": "~3.5.2",
    "electron-is-dev": "~2.0.0",
    "electron-serve": "~1.1.0",
    "electron-unhandled": "~3.0.2",
    "electron-updater": "~4.3.9",
    "electron-window-state": "~5.0.3",
    "express": "^4.17.1",
    "fix-path": "^4.0.0",
    "fkill": "^7.2.1",
    "socket.io": "^4.2.0"
  },
  "devDependencies": {
    "electron": "~13.1.9",
    "electron-builder": "^22.14.5",
    "electron-rebuild": "~2.3.5",
    "typescript": "~4.3.5"
  },
  "keywords": [
    "capacitor",
    "electron"
  ]
}
